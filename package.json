{
        "name": "vite-react",
        "private": true,
        "version": "0.0.0",
        "type": "module",
        "author": {
                "name": "abhishek zambare",
                "email": "abhishekzambare@gmail.com",
                "url": "https://abhishekzambare.vercel.app/"
        },
        "scripts": {
                "dev": "vite",
                "build": "tsc -b && vite build",
                "lint": "eslint .",
                "preview": "vite preview",
                "lint:fix": "eslint . --fix",
                "format": "prettier --check . --config ./.prettierrc.json",
                "format:fix": "prettier --write . --config ./.prettierrc.json",
                "rm:all": "rm -rf node_modules .next out dist build",
                "re:start": "yarn rm:all && yarn install && yarn dev",
                "re:build": "yarn rm:all && yarn install && yarn build",
                "dev:host": "vite --host"
        },
        "dependencies": {
                "@emotion/react": "^11.14.0",
                "@emotion/styled": "^11.14.1",
                "@mui/icons-material": "^7.3.4",
                "@mui/material": "^7.3.4",
                "eslint-config-prettier": "^10.1.8",
                "eslint-plugin-prettier": "^5.5.4",
                "eslint-plugin-react": "^7.37.5",
                "motion": "^12.23.24",
                "prettier": "^3.6.2",
                "react": "^19.2.0",
                "react-dom": "^19.2.0"
        },
        "devDependencies": {
                "@eslint/js": "^9.37.0",
                "@types/react": "^19.2.2",
                "@types/react-dom": "^19.2.2",
                "@vitejs/plugin-react": "^5.0.4",
                "babel-plugin-react-compiler": "^19.1.0-rc.3",
                "eslint": "^9.37.0",
                "eslint-plugin-react-hooks": "^7.0.0",
                "eslint-plugin-react-refresh": "^0.4.23",
                "globals": "^16.4.0",
                "typescript": "~5.9.3",
                "typescript-eslint": "^8.46.1",
                "vite": "npm:rolldown-vite@7.1.17",
                "vite-plugin-checker": "^0.11.0"
        },
        "resolutions": {
                "vite": "npm:rolldown-vite@7.1.12"
        }
}
